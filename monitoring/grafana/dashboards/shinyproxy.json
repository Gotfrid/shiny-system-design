      "pluginVersion": "12.0.0",
      "targets": [
        {
          "editorMode": "code",
          "format": "time_series",
          "rawQuery": true,
          "rawSql": "WITH timerange AS (\n  SELECT date_trunc('minute', series) as time\n  FROM generate_series($__timeFrom(), $__timeTo(), '1 minute'::interval) series\n),\n\nproxy_starts AS (\n  select date_trunc('minute', event_time) as time, 1 as action\n  from event\n  where type = 'ProxyStart'\n),\n\nproxy_starts_agg AS (\n  select time, sum(action) as action\n  from proxy_starts\n  group by time\n),\n\nproxy_stops AS (\n  select date_trunc('minute', event_time) as time, 1 as action\n  from event\n  where type = 'ProxyStop'\n),\n\nproxy_stops_agg AS (\n  select time, sum(action) as action\n  from proxy_stops\n  group by time\n),\n\naggregated AS (\n  SELECT\n    $__timeGroup(a.time, 1m) as time,\n    SUM(COALESCE(b.action, 0)) as proxy_starts,\n    SUM(COALESCE(c.action, 0)) as proxy_stops\n  FROM timerange a\n  LEFT JOIN proxy_starts_agg b ON a.time = b.time\n  LEFT JOIN proxy_stops_agg c ON a.time = c.time\n  GROUP BY 1\n)\n\nSELECT\n  time,\n  SUM(proxy_starts) OVER(ORDER BY time) - SUM(proxy_stops) OVER(ORDER BY time) as running\nFROM aggregated\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Running Applications",
      "type": "timeseries"
