          "rawQuery": true,
          "rawSql": "with timerange as (\n  select date_trunc('day', series) as time\n  from generate_series($__timeFrom(), $__timeTo(), '1 day'::interval) series\n),\nlogins as (\n  select session, min(time) as time\n  from event_log\n  where type = 'login'\n  group by session\n),\nlogouts as (\n  select session, min(time) as time\n  from event_log\n  where type = 'logout'\n  group by session\n),\navg_session_duration as (\n  select date_trunc('day', b.time) as time, extract(epoch from avg((b.time - a.time))) as session_duration\n  from logins a\n  join logouts b on a.session = b.session\n  group by 1\n)\n\nselect a.time, coalesce(b.session_duration, 0) as avg_session_duration\nfrom timerange a\nleft join avg_session_duration b\non a.time = b.time\n",
